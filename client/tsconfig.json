{
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"moduleResolution": "node",
		"outDir": "out",
		"rootDir": "src",
		"sourceMap": true,
		"strict": true,
		"strictPropertyInitialization": false,
		"strictNullChecks": false,
	},
	"include": [
		"src",
	],
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}